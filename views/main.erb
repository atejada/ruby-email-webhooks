<style>
.webhook {
	padding: 10px;
    margin: 5px;
    background-color: #f3f3f3
}
.h1 {
    text-align: center;
}
</style>
<title>Nylas Webhooks</title>
<h1 class='h1'>Webhooks</h1>
<% messages_opened.each do |item| %>
  <div class='messages_opened'>
    <p><b>Id:</b> <%= item.id %> | <b>Date:</b> <%= item.date %> | <b>Label:</b> <%= item.label %> | 
          <b>Subject:</b> <%= item.subject %> | <b>Count:</b> <%= item.count %></p>
  </div>
<% end %>
